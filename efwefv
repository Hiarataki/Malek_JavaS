  var liczba1=prompt("x=","");
  var liczba2=prompt("y=","");
  var x_m=parseFloat(liczba1);
  var y_m=parseFloat(liczba2);
  
document.write( "wczytane liczby "+"  x= "+x_m);
document.write("y="+y_m+"<br>");
document.write("("+x_m+"+"+y_m+")^3=");
document.write(x_m+"^3"+"");
document.write(x_m+ " *3"+" <br>");
document.write(y_m+ " *3"+" ");

</script>
